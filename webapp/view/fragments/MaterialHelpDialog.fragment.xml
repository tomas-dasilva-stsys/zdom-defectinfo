    <ValueHelpDialog
        title="{i18n>materialSelection}"
        ok=".onValueHelpOkPress"
        cancel=".onExitDialog"
        key="Material"
        descriptionKey="Name"
        supportMultiselect="false"
    >
        <filterBar>
            <fb:FilterBar search="onFilterBarSearch">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Material"
                        label="{i18n>material}"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input name="Material" submit="onSubmitFilter"/>
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Description"
                        label="{i18n>materialDescription}"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input name="Description" submit="onSubmitFilter"/>
                        </fb:control>
                    </fb:FilterGroupItem>
